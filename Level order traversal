#include <stdio.h>
#include <stdlib.h>

// Definition of binary tree node
struct Node
{
    int data;
    struct Node *left;
    struct Node *right;
};

// Queue structure for level order traversal
struct Queue
{
    int front, rear;
    struct Node *arr[100];
};

// Create a new tree node
struct Node* createNode(int value)
{
    struct Node* newNode = (struct Node*)malloc(sizeof(struct Node));
    newNode->data = value;
    newNode->left = NULL;
    newNode->right = NULL;
    return newNode;
}

// Initialize queue
void initQueue(struct Queue *q)
{
    q->front = q->rear = -1;
}

// Enqueue
void enqueue(struct Queue *q, struct Node* item)
{
    if (q->rear == 99)
        return;
    if (q->front == -1)
        q->front = 0;
    q->arr[++q->rear] = item;
}

// Dequeue
struct Node* dequeue(struct Queue *q)
{
    if (q->front == -1)
        return NULL;

    struct Node* temp = q->arr[q->front];
    if (q->front == q->rear)
        q->front = q->rear = -1;
    else
        q->front++;
    return temp;
}

// Level Order Traversal
void levelOrder(struct Node* root)
{
    struct Queue q;
    struct Node* temp;

    if (root == NULL)
        return;

    initQueue(&q);
    enqueue(&q, root);

    while (q.front != -1)
    {
        temp = dequeue(&q);
        printf("%d ", temp->data);

        if (temp->left != NULL)
            enqueue(&q, temp->left);
        if (temp->right != NULL)
            enqueue(&q, temp->right);
    }
}

int main()
{
    // Creating binary tree
    struct Node* root = createNode(1);
    root->left = createNode(2);
    root->right = createNode(3);
    root->left->left = createNode(4);
    root->left->right = createNode(5);

    printf("Level Order Traversal: ");
    levelOrder(root);

    return 0;
}
